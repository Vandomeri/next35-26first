// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model User {
  id       Int    @id @default(autoincrement())
  username String
  email    String
  password String
  age      Int
}

model Product {
  id    Int    @id @default(autoincrement())
  title String
  price String
}
